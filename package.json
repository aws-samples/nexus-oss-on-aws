{
  "name": "sonatype-nexus3",
  "version": "0.1.0",
  "bin": {
    "sonatype-nexus3": "bin/sonatype-nexus3.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "init": "npm run init-deps && npx cdk bootstrap",
    "init-deps": "npm i",
    "deploy": "npx cdk deploy",
    "cleanup": "npx cdk destroy -c createNewVpc=true",
    "release": "npm run build && npm run test"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.74.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "aws-cdk": "1.74.0",
    "jest": "^26.6.3",
    "lodash": ">=4.17.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "~4.1.2"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.74.0",
    "@aws-cdk/aws-efs": "^1.74.0",
    "@aws-cdk/aws-eks": "^1.74.0",
    "@aws-cdk/aws-lambda-python": "^1.74.0",
    "@aws-cdk/aws-route53": "^1.74.0",
    "@aws-cdk/core": "1.74.0",
    "js-yaml": "^3.14.0",
    "source-map-support": "^0.5.16",
    "sync-request": "^6.1.0"
  }
}
